<!DOCTYPE html>
<html lang="en">
    <head>
        <% include ../commons/head.ejs %>
    <link href="/phone/css/pages/this-is-global-file-common.css" rel="stylesheet"><link href="/phone/css/pages/setting.css" rel="stylesheet"></head>
    <body>
        <% include ../commons/top.ejs %>
        <div class="g-wrap">
            <% include ../commons/header.ejs %>
            <div class="g-body">
                <!--page start-->
                <div class="g-list">
                    <a href="<%= dataInfo.routes['password-find'].route %>" class="g-list-row">
                        <div class="g-list-wrap">
                            <div class="g-list-wrap">
                                <div class="g-list-icon iconfont icon-dev"></div>
                                <div class="g-list-text">忘记密码,去找回</div>
                            </div>
                            <div class="g-list-wrap">
                                <div class="g-list-more iconfont icon-more"></div>
                            </div>
                        </div>
                    </a>
                    <a href="<%= dataInfo.routes['password-reset'].route %>" class="g-list-row">
                        <div class="g-list-wrap">
                            <div class="g-list-wrap">
                                <div class="g-list-icon iconfont icon-dev"></div>
                                <div class="g-list-text">修改密码,去重置</div>
                            </div>
                            <div class="g-list-wrap">
                                <div class="g-list-more iconfont icon-more"></div>
                            </div>
                        </div>
                    </a>
                    <div class="g-list-row page-logout">
                        <div class="g-list-wrap">
                            <div class="g-list-wrap">
                                <div class="g-list-icon iconfont icon-dev"></div>
                                <div class="g-list-text">退出账号,去确认</div>
                            </div>
                            <div class="g-list-wrap">
                                <div class="g-list-more iconfont icon-more"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--page end-->
            </div>
            <% include ../commons/footer.ejs %>
        </div>
        <% include ../commons/bottom.ejs %>
    <script type="text/javascript" src="/phone/js/pages/this-is-global-file-manifest.js"></script><script type="text/javascript" src="/phone/js/pages/this-is-global-file-vendor.js"></script><script type="text/javascript" src="/phone/js/pages/this-is-global-file-common.js"></script><script type="text/javascript" src="/phone/js/pages/setting.js"></script></body>
</html>
