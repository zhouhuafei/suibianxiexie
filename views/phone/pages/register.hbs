<!DOCTYPE html>
<html lang="en">
    <head>
        {{>phone/partials/head}}
    <link href="/dist/phone/css/pages/this-is-global-file-global.css" rel="stylesheet"><link href="/dist/phone/css/pages/register.css" rel="stylesheet"></head>
    <body>
        <div class="g-wrap">
            {{>phone/partials/header}}
            <!--page start-->
            <div class="g-body">
                <!--结构待续...-->
                <div class="page-logo">
                    <div class="iconfont icon-logo"></div>
                </div>
                <div class="page-canvas"></div>
                <div class="page-input-wrap">
                    <label class="g-input">
                        <span class="g-input-text">账号:</span>
                        <input class="g-input-input g-validate-form" data-validate="no-space no-zero yes-integer" data-hint="本项必填 本项不可为零 请输入整数" name="username" id="username" value="" placeholder="请输入邮箱">
                        <span class="g-input-hint iconfont icon-bitian"></span>
                    </label>
                </div>
                <div class="page-input-wrap">
                    <label class="g-input">
                        <span class="page-verify-code">
                            <span class="g-button">获取验证码</span>
                        </span>
                        <input class="g-input-input g-validate-form" data-validate="no-space no-zero yes-integer" data-hint="本项必填 本项不可为零 请输入整数" name="verifyCode" id="verify-code" value="" placeholder="请输入验证码">
                        <span class="g-input-hint iconfont icon-bitian"></span>
                    </label>
                </div>
                <div class="page-input-wrap">
                    <label class="g-input">
                        <span class="g-input-text">密码:</span>
                        <input class="g-input-input" type="password" name="password" id="password" value="" placeholder="请输入密码">
                        <span class="g-input-icon iconfont icon-keshi"></span>
                        <span class="g-input-icon iconfont icon-qingkong"></span>
                        <span class="g-input-hint iconfont icon-bitian"></span>
                    </label>
                </div>
                <div class="g-button js-register">
                    <div class="g-button-text">立即注册</div>
                </div>
                <div class="page-guide-wrap">
                    <a href="{{pageInfo.routes.login.route}}" class="page-guide-item">
                        <span>已有账号,去登陆</span>
                        <span class="iconfont icon-youjiantou"></span>
                    </a>
                    <a href="{{pageInfo.routes.password-find.route}}" class="page-guide-item">
                        <span>忘记密码,去找回</span>
                        <span class="iconfont icon-youjiantou"></span>
                    </a>
                </div>
            </div>
            <!--page end-->
            {{>phone/partials/footer}}
        </div>
    <script type="text/javascript" src="/dist/phone/js/pages/this-is-global-file-manifest.js"></script><script type="text/javascript" src="/dist/phone/js/pages/this-is-global-file-vendor.js"></script><script type="text/javascript" src="/dist/phone/js/pages/this-is-global-file-global.js"></script><script type="text/javascript" src="/dist/phone/js/pages/register.js"></script></body>
</html>
