    "scripts": {
        "codeLint": "cross-env NODE_ENV=production eslint --ext .js,.vue,.html ./",
        "codeFix": "cross-env NODE_ENV=production eslint --fix --ext .js,.vue,.html ./",
        "precommit": "npm run codeLint",
        "serverDev": "nodemon app.js",
        "serverDevDocker": "nodemon app.js --legacy-watch",
        "serverStart": "pm2 start pm2.json",
        "serverStartDocker": "pm2 start pm2.json --no-daemon",
        "serverStop": "pm2 stop all",
        "serverRestart": "pm2 restart all",
        "serverReload": "pm2 reload all",
        "serverSave": "pm2 save",
        "serverStartup": "pm2 startup",
        "serverProxy": "browser-sync start --proxy 127.0.0.1:5551 --files dist/assets/**/*.*",
        "appWebpack": "webpack --colors --profile --progress --display-modules --config webpack.config.app.js",
        "appH5Dev": "cross-env NODE_ENV=development-app/h5 npm run appWebpack",
        "appH5Build": "cross-env NODE_ENV=production-app/h5 npm run appWebpack",
        "appPcDev": "cross-env NODE_ENV=development-app/pc npm run appWebpack",
        "appPcBuild": "cross-env NODE_ENV=production-app/pc npm run appWebpack",
        "appAdminDev": "cross-env NODE_ENV=development-app/admin npm run appWebpack",
        "appAdminBuild": "cross-env NODE_ENV=production-app/admin npm run appWebpack",
        "vueWebpack": "webpack --colors --profile --progress --display-modules --config webpack.config.vue.js",
        "vueHotWebpack": "webpack-dev-server --inline --config webpack.config.vue.js",
        "vueH5Dev": "cross-env NODE_ENV=development-vue/h5 npm run vueWebpack",
        "vueH5DevHot": "cross-env NODE_ENV=development-vue/h5 npm run vueHotWebpack",
