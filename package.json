        "vuePcBuild": "cross-env NODE_ENV=production-vue/pc npm run vueWebpack",
        "vueAdminDev": "cross-env NODE_ENV=development-vue/admin npm run vueWebpack",
        "vueAdminDevHot": "cross-env NODE_ENV=development-vue/admin npm run vueHotWebpack",
        "vueAdminBuild": "cross-env NODE_ENV=production-vue/admin npm run vueWebpack"
